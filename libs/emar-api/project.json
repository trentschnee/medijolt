{
  "name": "emar-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/emar-api/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [ "{options.outputFile}" ],
      "options": {
        "lintFilePatterns": [ "libs/emar-api/**/*.ts" ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [ "{workspaceRoot}/coverage/{projectRoot}" ],
      "options": {
        "jestConfig": "libs/emar-api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [ ]
}
