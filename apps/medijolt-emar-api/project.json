{
  "name": "medijolt-emar-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/medijolt-emar-api/src",
  "projectType": "application",
  "targets": {
    "postgres-start": {
      "executor": "@plugins/mongodb-executors:mongodb-start",
      "options": {
        "dockerComposeFilePath": "apps/medijolt-emar-api/docker/docker-compose.yml"
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [ "{options.outputPath}" ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/emar-api",
        "main": "apps/medijolt-emar-api/src/main.ts",
        "tsConfig": "apps/medijolt-emar-api/tsconfig.app.json",
        "tsPlugins": [
          {
            "name": "@nestjs/swagger/plugin",
            "options": {
              "dtoFileNameSuffix": [ ".entity.ts", ".dto.ts" ],
              "controllerFileNameSuffix": [ ".controller.ts" ],
              "classValidatorShim": true,
              "dtoKeyOfComment": "description",
              "controllerKeyOfComment": "description",
              "introspectComments": true
            }
          }
        ],
        "assets": [ "apps/medijolt-emar-api/src/assets" ],
        "isolatedConfig": true,
        "webpackConfig": "apps/medijolt-emar-api/webpack.config.js"
      },
      "configurations": {
        "development": { },
        "production": { }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "medijolt-emar-api:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "medijolt-emar-api:build"
        },
        "production": {
          "buildTarget": "medijolt-emar-api:build"
        }
      },
      "dependsOn": [
        {
          "target": "postgres-start",
          "projects": "self"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [ "{options.outputFile}" ],
      "options": {
        "lintFilePatterns": [ "apps/medijolt-emar-api/**/*.ts" ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [ "{workspaceRoot}/coverage/{projectRoot}" ],
      "options": {
        "jestConfig": "apps/medijolt-emar-api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [ ]
}
