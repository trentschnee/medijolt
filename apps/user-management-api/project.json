{
  "name": "user-management-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/user-management-api/src",
  "projectType": "application",
  "targets": {

    "postgres-start": {
      "executor": "@plugins/mongodb-executors:mongodb-start",
      "options": {
        "dockerComposeFilePath": "docker/docker-compose.yml"
      }
    },
    "postgres-stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker-compose -f apps/user-management-api/docker/docker-compose.yml down"
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [ "{options.outputPath}" ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/user-management-api",
        "main": "apps/user-management-api/src/main.ts",
        "tsConfig": "apps/user-management-api/tsconfig.app.json",
        "assets": [ "apps/user-management-api/src/assets" ],
        "isolatedConfig": true,
        "webpackConfig": "apps/user-management-api/webpack.config.js",
        "tsPlugins": [
          {
            "name": "@nestjs/swagger/plugin",
            "options": {
              "dtoFileNameSuffix": [ ".entity.ts", ".dto.ts" ],
              "controllerFileNameSuffix": [ ".controller.ts" ],
              "classValidatorShim": true,
              "dtoKeyOfComment": "description",
              "controllerKeyOfComment": "description",
              "introspectComments": true
            }
          }
        ]
      },
      "configurations": {
        "development": {
        },
        "production": { }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "user-management-api:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "user-management-api:build:development"
        },
        "production": {
          "buildTarget": "user-management-api:build:production"
        }
      },
      "dependsOn": [
        {
          "target": "postgres-start",
          "projects": "self"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [ "{options.outputFile}" ],
      "options": {
        "lintFilePatterns": [ "apps/user-management-api/**/*.ts" ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [ "{workspaceRoot}/coverage/{projectRoot}" ],
      "options": {
        "jestConfig": "apps/user-management-api/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [ ]
}
